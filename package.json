{
  "name": "obsidian-darkslide",
  "version": "1.1.1",
  "description": "DarkSlide - Dial in the perfect brightness for any Obsidian theme",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "deploy": "npm run build && pwsh ./deploy.ps1"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "brightness",
    "theme",
    "background",
    "darkslide"
  ],
  "author": "ThorGarden",
  "license": "SEE LICENSE IN LICENSE.md",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}